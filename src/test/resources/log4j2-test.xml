<?xml version="1.0" encoding="UTF-8"?>
<Configuration package="log4j.test" status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="ERROR">
            <AppenderRef ref="Console"/>
        </Root>
        <Logger name="beam.agentsim.agents.rideHail.RideHailDebugEventHandler" level="DEBUG">
            <AppenderRef ref="Console"/>
        </Logger>
        <!--<Logger name="beam" level="DEBUG"-->
        <!--additivity="false">-->
        <!--<AppenderRef ref="Console"/>-->
        <!--</Logger>-->
    </Loggers>
</Configuration>