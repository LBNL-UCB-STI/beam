<?xml version="1.0" encoding="UTF-8" ?>
<households
        xmlns="http://www.matsim.org/files/dtd"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.matsim.org/files/dtd http://www.matsim.org/files/dtd/households_v1.0.xsd">

    <household id="1">
        <members>
            <personId refId="1" />
            <personId refId="2" />
            <personId refId="3" />
        </members>
        <vehicles>
            <vehicleDefinitionId refId="2" />
            <vehicleDefinitionId refId="3" />
        </vehicles>
        <income currency="eur" period="month">50000</income>
    </household>

    <household id="2">
        <members>
            <personId refId="4" />
            <personId refId="5" />
        </members>
        <vehicles>
            <vehicleDefinitionId refId="4" />
        </vehicles>
        <income currency="eur" period="day">
            1000
        </income>
    </household>

    <household id="3">
        <vehicles>
            <vehicleDefinitionId refId="1" />
        </vehicles>
    </household>
</households>